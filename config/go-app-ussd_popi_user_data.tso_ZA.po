#: go-app-ussd_popi_user_data.js:99
msgid ""
"Personal info:\n"
"Phone Number: {{msisdn}}\n"
"ID Number: {{id}}\n"
"Date of Birth: {{dob}}\n"
"Channel: {{channel}}\n"
"Language: {{lang}}"
msgstr ""
"Vuxokoxoko bya wena:\n"
"Nomboro ya Foni: {{msisdn}}\n"
"Nomboro ya Pasi: {{id}}\n"
"Siku ro Velekiwa: {{dob}}\n"
"Chanele: {{channel}}\n"
"Ririmi: {{lang}}"

#: go-app-ussd_popi_user_data.js:113
msgid ""
"Personal info:\n"
"Phone Number: {{msisdn}}\n"
"Origin of Passport: {{passport_or}}\n"
"Passport: {{passport_num}}\n"
"Date of Birth: {{dob}}\n"
"Channel: {{channel}}\n"
"Language: {{lang}}"
