#: go-app-ussd_popi_user_data.js:145 go-app-ussd_popi_user_data.js:959
#: go-app-ussd_popi_user_data.js:1011
msgid "WhatsApp"
msgstr "I-WhatsApp"

#: go-app-ussd_popi_user_data.js:288
msgid "What would you like to do?"
msgstr "Ufisa ukwenzani?"

#: go-app-ussd_popi_user_data.js:290
msgid "See my personal info"
msgstr "Ukubheka ulwazi lwami"

#: go-app-ussd_popi_user_data.js:291
msgid "Change my info"
msgstr "Ukushintsha ulwazi lwami"

#: go-app-ussd_popi_user_data.js:292
msgid "Request to delete my info"
msgstr "Ukusula ulwazi lwami"

#: go-app-ussd_popi_user_data.js:303
msgid "More"
msgstr "Okunye"

#: go-app-ussd_popi_user_data.js:304
msgid "Back"
msgstr "Emuva"

#: go-app-ussd_popi_user_data.js:305
msgid "Main Menu"
msgstr "Imenyu Enkulu"

#: go-app-ussd_popi_user_data.js:318
msgid "Use a different phone number"
msgstr "Ukusebenzisa enye inombolo yefoni"

#: go-app-ussd_popi_user_data.js:319
msgid "Update my language choice"
msgstr "Ukubukeza ulimi lwami"

#: go-app-ussd_popi_user_data.js:320
msgid "Update my identification"
msgstr "Ukubukeza inkomba yami"

#: go-app-ussd_popi_user_data.js:327
msgid "Receive messages over {{channel}}"
msgstr "Thola imiyalezo nge {{channel}}"

#: go-app-ussd_popi_user_data.js:334
msgid "What would you like to change? To change your due date, visit a clinic"
msgstr "Ufisa ukushintshani? Ukushintsha usuku lokubeletha, iya eklinikhi"

#: go-app-ussd_popi_user_data.js:346
msgid ""
"MomConnect will automatically delete your personal information 7 years and 9 "
"months after you registered. Do you want us to delete it now?"
msgstr ""
"I-MomConnect izozisulela ulwazi lakho emuva kweminyaka ewu-7 nezinyanga "
"eziwu-9 ubhalisile. Noma ufisa siyisula manje?"

#: go-app-ussd_popi_user_data.js:350 go-app-ussd_popi_user_data.js:916
msgid "Yes"
msgstr "Yebo"

#: go-app-ussd_popi_user_data.js:351
msgid "No"
msgstr "Cha"

#: go-app-ussd_popi_user_data.js:382
msgid ""
"Thank you. Your info has been updated. You will now receive messages from "
"MomConnect via {{channel}}."
msgstr ""
"Siyabonga. Imininingwane yakho ibukeziwe. Sewuzothola imiyalezo evela "
"kuMomConnect nge {{channel}}."

#: go-app-ussd_popi_user_data.js:389 go-app-ussd_popi_user_data.js:446
#: go-app-ussd_popi_user_data.js:556 go-app-ussd_popi_user_data.js:667
msgid "Update my other info"
msgstr "Bukeza eminye iminingwane yami"

#: go-app-ussd_popi_user_data.js:396
msgid "Choose a language for your messages:"
msgstr "Khetha ulimi lomyalezo wakho:"

#: go-app-ussd_popi_user_data.js:399 go-app-ussd_popi_user_data.js:840
msgid "isiZulu"
msgstr "isiZulu"

#: go-app-ussd_popi_user_data.js:400 go-app-ussd_popi_user_data.js:841
msgid "isiXhosa"
msgstr "isiXhosa"

#: go-app-ussd_popi_user_data.js:401 go-app-ussd_popi_user_data.js:842
msgid "Afrikaans"
msgstr "isiBhunu"

#: go-app-ussd_popi_user_data.js:402 go-app-ussd_popi_user_data.js:843
msgid "English"
msgstr "isNgisi"

#: go-app-ussd_popi_user_data.js:403 go-app-ussd_popi_user_data.js:844
msgid "Sesotho sa Leboa"
msgstr "Sepedi"

#: go-app-ussd_popi_user_data.js:404 go-app-ussd_popi_user_data.js:845
msgid "Setswana"
msgstr "Setswana"

#: go-app-ussd_popi_user_data.js:405 go-app-ussd_popi_user_data.js:846
msgid "Sesotho"
msgstr "isiSuthu"

#: go-app-ussd_popi_user_data.js:406 go-app-ussd_popi_user_data.js:847
msgid "Xitsonga"
msgstr "Xitsonga"

#: go-app-ussd_popi_user_data.js:407 go-app-ussd_popi_user_data.js:848
msgid "siSwati"
msgstr "isiSwati"

#: go-app-ussd_popi_user_data.js:408 go-app-ussd_popi_user_data.js:849
msgid "Tshivenda"
msgstr "Tshivenda"

#: go-app-ussd_popi_user_data.js:409 go-app-ussd_popi_user_data.js:850
msgid "isiNdebele"
msgstr "isiNdebele"

#: go-app-ussd_popi_user_data.js:438
msgid ""
"Thank you. Your info has been updated. You will now receive messages from "
"MomConnect in {{language}}."
msgstr ""
"Siyabonga. Imininingwane yakho ibukeziwe. Sewuzothola imiyalezo evela "
"kuMomConnect nge {{language}}."

#: go-app-ussd_popi_user_data.js:453
msgid "What kind of identification do you have?"
msgstr "Uneyiphi inkomba?"

#: go-app-ussd_popi_user_data.js:455 go-app-ussd_popi_user_data.js:539
msgid "South African ID"
msgstr "I-ID yaseNingizimu Afrika"

#: go-app-ussd_popi_user_data.js:456 go-app-ussd_popi_user_data.js:542
msgid "Passport"
msgstr "Iphasiphothi"

#: go-app-ussd_popi_user_data.js:466
msgid "Thank you. Please enter your ID number. eg. 8805100273098"
msgstr "Siyabonga. Sicela ufake i-ID yakho (e.g. 8805100273098)"

#: go-app-ussd_popi_user_data.js:472
msgid "Invalid ID number. Please re-enter"
msgstr "Inombolo kaMazisi engasebenzi. Sicela uyifake kabusha"

#: go-app-ussd_popi_user_data.js:481
msgid "What is the country of origin of the passport?"
msgstr "Iphasiphothi yakho eyeliphi izwe?"

#: go-app-ussd_popi_user_data.js:483
msgid "Zimbabwe"
msgstr "Zimbabwe"

#: go-app-ussd_popi_user_data.js:484
msgid "Mozambique"
msgstr "Mozambique"

#: go-app-ussd_popi_user_data.js:485
msgid "Malawi"
msgstr "Malawi"

#: go-app-ussd_popi_user_data.js:486
msgid "Nigeria"
msgstr "Nigeria"

#: go-app-ussd_popi_user_data.js:487
msgid "DRC"
msgstr "DRC"

#: go-app-ussd_popi_user_data.js:488
msgid "Somalia"
msgstr "Somalia"

#: go-app-ussd_popi_user_data.js:489
msgid "Other"
msgstr "Okunye"

#: go-app-ussd_popi_user_data.js:496
msgid ""
"There was an error in your entry. Please carefully enter the passport number "
"again."
msgstr ""
"Kunephutha kulokho okufakile. Sicela uphinde ufake inombolo yakho "
"yephasiphothi ngokucophelela."

#: go-app-ussd_popi_user_data.js:498
msgid "Please enter the passport number:"
msgstr "Siyabonga. Faka i-No. yephasiphothi yakho"

#: go-app-ussd_popi_user_data.js:547
msgid ""
"Thank you. Your info has been updated. Your registered identification is "
"{{identification_type}} {{identification_number}}."
msgstr ""
"Siyabonga. Imininingwane yakho ibukeziwe. Inkomba yakho ebhalisiwe ithi "
"{{identification_type}} {{identification_number}}."

#: go-app-ussd_popi_user_data.js:563
msgid ""
"Please enter the new phone number we should use to send you messages eg. "
"0813547654"
msgstr ""
"Sicela ufake inombolo yefoni entsha esizokuthumelela imiyalezo kuyo isib. "
"0813547654"

#: go-app-ussd_popi_user_data.js:569
msgid "Invalid phone number. Please re-enter (with no spaces)"
msgstr ""
"Inombolo yocingo engasebenzi. Sicela uyifake kabusha (ungazishiyi izikhala)"

#: go-app-ussd_popi_user_data.js:622
msgid ""
"Sorry, the number you are trying to move to already has an active "
"registration. To manage that registration, please redial from that number."
msgstr ""
"Uxolo, kukhona obhalise futhi osebenzisa inombolo ozama ukudlulela kuyo. "
"Ukuze uhlele lokho kubhalisa, sicela ushaye kabusha ngaleyo nombolo."

#: go-app-ussd_popi_user_data.js:659
msgid ""
"Thank you. Your info has been updated. You will now receive messages from "
"MomConnect via on phone number {{msisdn}}"
msgstr ""
"Siyabonga. Imininingwane yakho ibukeziwe. Sewuzothola imiyalezo evela "
"kuMomConnect ngenombolo yakho yocingo  {{msisdn}}"

#: go-app-ussd_popi_user_data.js:674
msgid "Your personal information stored on MomConnect has not been removed."
msgstr "Ulwazi oluqondene nawe olugcinwe iMomConnect alukasuswa."

#: go-app-ussd_popi_user_data.js:710
msgid ""
"Thank you. All your information will be deleted from MomConnect in the next "
"3 days."
msgstr ""
"Siyabonga. Lonke ulwazi lwakho luzosulwa i-MomConnect ezinsukwini ezingu 3 "
"ezizayo."

#: go-app-ussd_popi_user_data.js:719
msgid ""
"Sorry, the number you dialled with is not recognised. Dial in with the "
"number you use for MomConnect to change your details"
msgstr ""
"Uxolo, inombolo oshaye ngayo ayaziwa. Sishayele ngenombolo oyisebenzisela "
"iMomConnect ukushintsha imininingwane yakho"

#: go-app-ussd_popi_user_data.js:727
msgid "I don't have that SIM"
msgstr "Anginayo leyo-SIM"

#: go-app-ussd_popi_user_data.js:728 go-app-ussd_popi_user_data.js:781
#: go-app-ussd_popi_user_data.js:950
msgid "Exit"
msgstr "Phuma"

#: go-app-ussd_popi_user_data.js:735
msgid ""
"Thank you for using MomConnect. Dial *134*550*7# to see, change or delete "
"the your MomConnect information."
msgstr ""
"Siyabonga ngokusebenzisa iMomConnect. Dayela *134*550*7# ukubona, "
"ukushintsha noma ukusula imininingwane yakho yeMomConnect."

#: go-app-ussd_popi_user_data.js:743
msgid "Please enter the number you receive MomConnect messages on."
msgstr "Sicela ufake inombolo othola imiyalezo yeMomConnect kuyo."

#: go-app-ussd_popi_user_data.js:775
msgid ""
"Sorry we do not recognise that number. New to MomConnect?Please visit a "
"clinic to register. Made a mistake?"
msgstr ""
"Siyaxolisa asiyazi leyo nombolo. Umusha kuMomConnect? Sicela uye kobhalisa "
"eklinikhi. Wenze iphutha?"

#: go-app-ussd_popi_user_data.js:780 go-app-ussd_popi_user_data.js:949
msgid "Try again"
msgstr "Zama futhi"

#: go-app-ussd_popi_user_data.js:788
msgid ""
"Thank you. To change your mobile number we first need to verify your "
"identity. Please enter your SA ID number now."
msgstr ""
"Siyabonga. Ukushintsha inombolo yocingo yakho sidinga ukuqinisekisa ukuthi "
"nguwena. Sicela ufake inombolo yakho ye-ID yaseNingizimu Afrika manje."

#: go-app-ussd_popi_user_data.js:796
msgid ""
"Thank you. To change your mobile number we first need to verify your "
"identity. Please enter your passport number now."
msgstr ""
"Siyabonga. Ukushintsha inombolo yocingo yakho sidinga ukuqinisekisa ukuthi "
"nguwena. Sicela ufake inombolo yephasiphothi yakho manje."

#: go-app-ussd_popi_user_data.js:804
msgid ""
"Thank you. To change your mobile number we first need to verify your "
"identity. Please enter your date of birth in the following format: dd*mm*yyyy"
msgstr ""
"Siyabonga. Ukushintsha inombolo yocingo yakho sidinga ukuqinisekisa ukuthi "
"nguwena. Sicela ufake usuku lokuzalwa ngendlela elandelayo: "
"usuku*inyanga*unyaka"

#: go-app-ussd_popi_user_data.js:826
msgid ""
"Sorry that is not the correct format. Please enter your date of birth in the "
"following format: dd*mm*yyyy. For example 19*05*1990"
msgstr ""
"Uxolo akusiyo indlela efanele leyo. Sicela ufake usuku lokuzalwa ngendlela "
"elandelayo: usuku*inyanga*unyaka. Isibonelo 19*05*1990"

#: go-app-ussd_popi_user_data.js:837
msgid "Thank you. Please select the language you receive message in:"
msgstr "Siyabonga. Sicela ukhethe ulimi othola imiyalezo ngalo:"

#: go-app-ussd_popi_user_data.js:887
msgid ""
"Sorry one or more of the answers you provided are incorrect. We are not able "
"to change your mobile number. Please visit the clinic to register your new "
"number."
msgstr ""
"Uxolo kunempendulo engalungile. Siyehluleka ukushintsha inombolo yakho "
"yocingo. Sicela uye kobhalisa inombolo yakho entsha eklinikhi."

#: go-app-ussd_popi_user_data.js:896
msgid ""
"Thank you. Please enter the new number you would like to use to receive "
"messages from MomConnect."
msgstr ""
"Siyabonga. Sicela ufake inombolo entsha othanda ukuthola imiyalezo "
"yeMomConnect kuyo."

#: go-app-ussd_popi_user_data.js:902
msgid ""
"Sorry the number you have entered is not valid. Please re-enter the mobile "
"number."
msgstr ""
"Uxolo inombolo oyifakile ayisebenzi. Sicela uyifake kabusha inombolo yocingo."

#: go-app-ussd_popi_user_data.js:911
msgid ""
"You have entered {{new_number}} as the new number you would like to receive "
"MomConnect messages on. Is this number correct?"
msgstr ""
"Sewufake {{new_number}} njengenombolo entsha ongathanda ukuthola imiyalezo "
"yeMomConnect kuyo. Ingabe le nombolo ilungile?"

#: go-app-ussd_popi_user_data.js:917
msgid "No - enter again"
msgstr "Cha - faka futhi"

#: go-app-ussd_popi_user_data.js:944
msgid ""
"Sorry the number you have entered is already associated with a MomConnect "
"account. Please try another number."
msgstr ""
"Uxolo inombolo oyifakile seyiyasebenza ku-akhawunti yeMomConnect. Sicela "
"uzame enye inombolo."

#: go-app-ussd_popi_user_data.js:957
msgid ""
"Thank you. How would you like to receive messages about you and your baby?"
msgstr ""
"Siyabonga. Ungathanda ukuyithola kanjani imiyalezo ngawe nengane yakho?"

#: go-app-ussd_popi_user_data.js:1017
msgid ""
"Your number has been changed successfully to {{msisdn}}. You will receive "
"messages on {{channel}}. Thank you for using MomConnect!"
msgstr ""
"Inombolo yakho ishintshekile yaba ngu {{msisdn}}. Uzothola imiyalezo ku "
"{{channel}}. Siyabonga ngokusebenzisa iMomConnect!"

